# Contest Manager Production Environment Configuration
# Copy this to .env.production before building for production

# Enable compact mode for Monday.com iframe
REACT_APP_COMPACT_MODE=true

# Enable Monday.com integration
REACT_APP_ENABLE_MONDAY=true
REACT_APP_MONDAY_BOARD_ID=your-monday-board-id

# AWS API Configuration - UPDATE THESE
REACT_APP_API_BASE_URL=https://your-api-gateway-id.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_API_KEY=your-production-api-key
REACT_APP_AWS_REGION=us-east-1

# Production Environment
REACT_APP_ENVIRONMENT=prod

# Customer Service Features (all enabled for production)
REACT_APP_ENABLE_TESTING_PANEL=true
REACT_APP_ENABLE_S3_BROWSER=true
REACT_APP_ENABLE_PYTHON_SCRIPTS=true
REACT_APP_ENABLE_SYSTEM_HEALTH=true

# Performance Optimization
REACT_APP_DEBUG=false
REACT_APP_SHOW_ADVANCED=false

# Generate source maps for debugging (set to false for smaller build)
GENERATE_SOURCEMAP=false
